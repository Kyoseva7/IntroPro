
public class Zad2 {

	public static void main(String[] args) {
		int[] Fibo = new int[12];
		Fibo[0] = 0;
		Fibo[1] = 1;
		System.out.println("Fibo[0] = 0");
		System.out.println("Fibo[1] = 1");
		
		for (int i = 2; i < Fibo.length; i++){
			Fibo[i] = (i - 1) + (i - 2);
			System.out.println("Fibo[" + i + "]" + " = " + Fibo[i]);
		}
	}

}
