import java.util.Scanner;

public class Zad2 {

	public static void main(String[] args) {
		Scanner input = new Scanner(System.in);
		System.out.println("Vuvedete 3 celi chisla: ");
		long num1 = input.nextInt();
		long num2 = input.nextInt();
		long num3 = input.nextInt();
		multiplyInt(num1, num2, num3);
		input.close();
	}
	public static long multiplyInt(long a, long b, long c) {
		long mul = a * b * c;
		return mul;
	}
}
