
public class Ex1 {

	public static void main(String[] args) {
		int x = 5;
		int y = 24;
		int z = x + y;
		int q = z * 5;
		System.out.print ("x = ");
		System.out.println (x);
		System.out.print ("y = ");
		System.out.println (y);
		System.out.println ("z = ");
		System.out.println (z);
		System.out.println ("q = ");
		System.out.println (q);
		

	}

}
