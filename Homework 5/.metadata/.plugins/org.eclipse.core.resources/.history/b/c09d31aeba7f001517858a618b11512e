import java.util.Scanner;

public class Zad3 {

	public static void main(String[] args) {
		Scanner input = new Scanner (System.in);
		System.out.println("Vuvedete 6 chisla: ");
		int a = input.nextInt();
		int b = input.nextInt();
		int c = input.nextInt();
		int d = input.nextInt();
		int e = input.nextInt();
		int f = input.nextInt();
		int min = 0;
		int max = 0;
		if (a < b) {
			min = a;
			max = b;
		} else {
			min = b;
			max = a;
		}
		if (min > c){
			min = c;
		}
		if (max < c){
			max = c;
		}
		if (min > d){
			min = d;
		}
		if (max < d){ 
			max = d;
		}
		if (min > e){
			min = e;
		}
		if (max < e){ 
			max = e;
		}
		if (min > f){
			min = f;
		}
		if (max < f){ 
			max = f;
		}
		System.out.print("Nai-malkoto chislo e: ");
		System.out.println(min);
		System.out.print("Nai-golqmoto chislo e: ");
		System.out.println(max);
		input.close();
	}
}
