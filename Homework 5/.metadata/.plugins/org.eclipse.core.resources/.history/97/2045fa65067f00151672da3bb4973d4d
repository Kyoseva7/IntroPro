
public class Zad5 {

	public static void main(String[] args) {
		int n = 5;
		int k = 9;
		long factN = 1;
		for (int i = 1; i <= n; i++){
			factN = factN * i;
		}
		long factK = 1;
		for (int j = 1; j <= k; j++){
			factK = factK * j;
		}
		int m = n - k;
		long factM = 1;
		for (int i = 1; i <= m; i++){
			factM = factM * i;
		}
		System.out.print("n! * k! / (n - k)! = ");
		System.out.println(factN * factK / factM);
	}
}
