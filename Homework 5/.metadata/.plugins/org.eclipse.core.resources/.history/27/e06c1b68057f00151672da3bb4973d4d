
public class Zad4 {

	public static void main(String[] args) {
		int a = 3;
		int b = 4;
		long factA = 1;
		for (int i = 1; i <= a; i++){
			factA = factA * i;
		}
		System.out.println(factA);
		long factB = 1;
		for (int j = 1; j <= b; j++){
			factB = factB * j;
		}
		System.out.println(factB);
		//System.out.print("a! / b! = ");
		//System.out.println(factA / factB);
	}
}
